<script>
    $(document).ready(function() {
        $('.skip-link').on('click', function() {
            $(this).toggleClass('active').siblings().removeClass('active');
            if($(this).hasClass('skip-nav')) $('.header_nav').toggleClass('active');
            else $('.header_nav').removeClass('active');
            if ($(this).hasClass('skip-search')) $('#search_mini_form').toggleClass('active');
            else $('#search_mini_form').removeClass('active');
        });

        var nodeListOfLinks = $('.product-name a'); // returns NodeList
        var arrayOfSlicedLinks = [];

        for (var i = 0; i < nodeListOfLinks.length; i += 1) {
            arrayOfSlicedLinks.push(nodeListOfLinks[i].innerHTML);
        }

        $.each(arrayOfSlicedLinks, function(index) {
            var slicedLink = arrayOfSlicedLinks[index].slice(0, 60);
            if (slicedLink.length < arrayOfSlicedLinks[index].length) {
                slicedLink += '...';
            }

            nodeListOfLinks[index].innerHTML = slicedLink;
        });
    });
</script>
